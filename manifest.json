{
  "name": "Window Cuts",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome extension to add keyboard shortcuts for managing multiple Chrome windows",
  "homepage_url": "https://github.com/frankcalise/windowcuts-chrome-extension",
  "author": {
    "name": "Frank Calise"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:frankcalise/windowcuts-chrome-extension.git"
  },
  "bugs": {
    "url": "https://github.com/frankcalise/windowcuts-chrome-extension/issues"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "switch_window_focus": {

    }
  },
  "keywords": [
    "chrome", "extension", "keyboard", "shortcuts", "manage", "windows"
  ]
}